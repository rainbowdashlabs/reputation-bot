{
  "baseBranches": ["development"],
  "dependencyDashboard": true,
  "labels": ["dependencies"],
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "groupName": "log4j",
      "matchPackagePrefixes": ["org.apache.logging.log4j"]
    },
    {
      "groupName": "sadu",
      "matchPackagePrefixes": ["de.chojo.sadu"]
    }
  ],
  "reviewers": [
    "dlsf",
    "RainbowDashLabs",
    "Taucher2003",
    "yannicklamprecht"
  ],
  "prConcurrentLimit": 3,
  "prHourlyLimit": 10
}
